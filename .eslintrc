{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-var": 2,
    "no-magic-numbers": ["error", {
      "ignore": [-1, 0, 1]
    }],
    "arrow-parens": [
      2,
      "always"
    ],
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "no-unreachable": 2,
    "prefer-const": 2,
    "prefer-arrow-callback": 2,
    "block-scoped-var": 2,
    "no-const-assign": 2,
    "no-console": 0,
    "no-debugger": 0,
    "no-unexpected-multiline": 2,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^ignore"
      }
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "curly": 2,
    "camelcase": [
      1,
      {
        "properties": "always"
      }
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-extend-native": 2,
    "guard-for-in": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "new-cap": 0,
    "no-caller": 2,
    "no-eval": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-inner-declarations": [
      2,
      "both"
    ],
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "new-parens": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "keyword-spacing": 2,
    "no-labels": 2,
    "block-spacing": [
      2,
      "always"
    ],
    "space-before-blocks": 2,
    "space-infix-ops": 2,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-dangle": ["error", "never"],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "eol-last": 2,
    "spaced-comment": [
      2,
      "always"
    ],
    "dot-notation": 2,
    "no-lone-blocks": 2,
    "no-redeclare": 2,
    "no-with": 2,
    "global-require": 0,
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "key-spacing": 2,
    "newline-after-var": [
      2,
      "always"
    ],
    "no-array-constructor": 2,
    "no-multiple-empty-lines": 2,
    "no-spaced-func": 2,
    "object-curly-spacing": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "arrow-spacing": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "commonjs": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "before": true
  }
}
